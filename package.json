{
  "name": "carto-api",
  "main": "./lib/carto-api",
  "version": "0.0.1",
  "description": "A suite of tools to interact with CARTO APIs (SQL, import, geocode...), from within Atom.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "carto-api:sql-get-csv",
      "carto-api:sql-get-geojson",
      "carto-api:geocode",
      "carto-api:import"
    ]
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "repository": "https://github.com/nerik/carto-api-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "cartodb": "^0.5.1"
  }
}
